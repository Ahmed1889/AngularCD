pipeline
{
		agent any
	          stages {
	              stage('pull) {
	                   steps{
	                      script{
	                      checkout([$class: 'GitSCM', branches: [[name: '*/master']],
	                         userRemoteConfigs: [[
	                             credentialsId: '',
	                             url: '']]])
	                             }
	                             }
	                             }
	          }
}
